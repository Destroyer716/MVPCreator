<idea-plugin>
  <id>com.kzg.plugin.mvpcreator</id>
  <name>MVPCreator</name>
  <version>1.0</version>
  <vendor email="3282324806@qq.com" url="http://www.baidu.com">NoCompany</vendor>

  <description>这是一款自动生成MVP模式代码的插件，能生成Activity，Fragment，Presenter，Contract，Model。
    安装后可以再“Code”菜单下的第一个找到它</description>

  <change-notes>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="Plugin.MVPCreator" class="action.MVPCreator" text="MVPCreator" description="MVPCreator">
      <add-to-group group-id="CodeMenu" anchor="first"/>
    </action>
  </actions>

</idea-plugin>